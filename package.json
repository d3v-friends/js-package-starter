{
  "name": "js-package-starter",
  "version": "0.0.1",
  "description": "js package starter",
  "scripts": {
    "build": "sh build.sh",
    "update": "pnpm update --recursive"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "typescript": "5.3.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "engines": {
    "node": ">=20.10.0"
  },
  "prettier": {
    "singleQuote": false,
    "tabWidth": 4,
    "printWidth": 140,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "semi": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}
