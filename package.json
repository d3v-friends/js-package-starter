{
  "name": "js-package-starter",
  "version": "0.0.1",
  "description": "js package starter",
  "scripts": {
    "build": "sh build.sh"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/node": "20.10.4",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "20.10.0"
  },
  "main": "./cjs/index.cjs`",
  "types": "./cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index.mjs",
      "require": "./cjs/index.cjs",
      "types": "./ts/index.ts"
    },
    "./*": {
      "import": "./mjs/*",
      "require": "./cjs/*",
      "types": "./ts/*"
    }
  }
}
